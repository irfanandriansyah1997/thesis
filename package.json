{
    "name": "@99/component-ninetynine",
    "version": "0.1.7",
    "description": "base component for 99 group website",
    "main": "index.js",
    "publishConfig": {
        "registry": "https://packages.99iddev.net"
    },
    "scripts": {
        "dev": "node_modules/.bin/webpack-dev-server --progress --config etc/webpack/webpack.config.dev.js",
        "storybook": "start-storybook -p 5000 -c src/.storybook",
        "test": "jest -w 2 --verbose src --cache",
        "test-without-coverage": "jest -w 2 --verbose src --cache --collectCoverage=false",
        "prelint": "node_modules/.bin/stylelint src",
        "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet --fix --color --no-ignore --max-warnings 100 --format pretty src",
        "precheck-code": "yarn run lint",
        "check-code": "yarn run test",
        "prebuild-documentation": "if [ -d documentation ]; then rm -Rf documentation; fi",
        "build-documentation": "build-storybook -c .storybook -o documentation -c src/.storybook",
        "build-css": "gulp --gulpfile etc/gulp/gulpfile.babel.js",
        "prebuild-asset": "if [ -d lib ]; then rm -Rf lib; fi",
        "build-asset": "rollup -c etc/rollup/rollup.config.js && yarn run build-css",
        "postbuild-asset": "tsc --build tsconfig.build.json && rm -rf dist",
        "build": "node_modules/.bin/webpack --config etc/webpack/webpack.config.production.js"
    },
    "repository": {
        "type": "git",
        "url": "ssh://git@github.com:urbanindo/style-guide.git",
        "directory": "urbaindo/component-ninetynine"
    },
    "keywords": [
        "react",
        "atomic design pattern"
    ],
    "author": "99.co",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/urbanindo/style-guide/issues"
    },
    "homepage": "https://github.com/urbanindo/style-guide#readme",
